#!/bin/bash
#########################################
################ LEVEL 0 ################
#########################################

# Primero hay que conectarnos mediante SSH 
# esto nos proporciona acceso al nivel 1.

ssh bandit0@bandit.labs.overthewire.org -p 2220
#password : bandit0

#########################################
################ LEVEL 1 ################
#########################################

cat readme 

#########################################
################ LEVEL 2 ################
#########################################

cat ./- 
#cat < -

#########################################
################ LEVEL 3 ################
#########################################

ls spaces\ in\ this\ filename 

#########################################
################ LEVEL 4 ################
#########################################

find . -type f
cat inhere/.hidden 

#########################################
################ LEVEL 5 ################
#########################################

file inhere/* #Sarching files human-readable
cat inhere/-file07 

#########################################
################ LEVEL 6 ################
#########################################

find . -type f -readable ! -executable -size 1033c | xargs cat | xargs 

#########################################
################ LEVEL 7 ################
#########################################

find / -group bandit6 -size 33c
cd /var/lib/dpkg/info/
cat bandit7.password 

#########################################
################ LEVEL 8 ################
#########################################

cat data.txt | grep millionth 

#########################################
################ LEVEL 9 ################
#########################################

sort data.txt | uniq -u #EN632PlfYiZbn3PhVK3XOGSlNInNE00t

#########################################
############### LEVEL 10 ################
#########################################

strings data.txt | grep "===*" | tail -n1 | awk 'NF{print $NF}' #G7w8LIi6J3kTb8A7j9LgrywtEUlyyp6s

# next => ssh bandit11@bandit.labs.overthewire.org -p 2220
